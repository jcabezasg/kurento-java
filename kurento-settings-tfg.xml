<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<localRepository/>
	<interactiveMode/>
	<usePluginRegistry/>
	<offline/>
	<pluginGroups/>
	<servers>
	    <server>
	      <id>kurento-snapshots</id>
	      <username>admin</username>
	      <password>admin</password>
	    </server>
	    <server>
	      <id>kurento-releases</id>
	      <username>admin</username>
	      <password>admin</password>
	    </server>
 	 </servers>


	<mirrors/>
	<proxies/>
	<profiles>
		<profile>
			<id>kurento-develop</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>

			<properties>
				<gpg.passphrase></gpg.passphrase>
			</properties>

			<repositories>
				<repository>
					<id>kurento-snapshots</id>
					<name>Kurento Snapshot Repository</name>
					<url>http://repo.maven.apache.org/maven2/org/kurento/</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
                      	<updatePolicy>always</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>kurento-releases</id>
					<name>Kurento Repository</name>
					<url>https://repo.maven.apache.org/maven2/org/kurento/</url>
					<releases>
						<enabled>true</enabled>
                      	<updatePolicy>always</updatePolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
			</repositories>
            <pluginRepositories>
              <pluginRepository>
                  <id>kurento-snapshots</id>
                  <name>Kurento Snapshot Repository</name>
                  <url>http://repo.maven.apache.org/maven2/org/kurento/</url>
                  <releases>
                      <enabled>false</enabled>
                  </releases>
                  <snapshots>
                      <enabled>true</enabled>
                      <updatePolicy>always</updatePolicy>
                  </snapshots>
              </pluginRepository>
              <pluginRepository>
                  <id>kurento-releases</id>
                  <name>Kurento Repository</name>
                  <url>https://repo.maven.apache.org/maven2/org/kurento/</url>
                  <releases>
                      <enabled>true</enabled>
                      <updatePolicy>always</updatePolicy>
                  </releases>
                  <snapshots>
                      <enabled>false</enabled>
                  </snapshots>
              </pluginRepository>
          </pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles/>
</settings>

