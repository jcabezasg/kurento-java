<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<localRepository/>
	<interactiveMode/>
	<usePluginRegistry/>
	<offline/>
	<pluginGroups/>
	<servers>
	    <server>
	      <id>${env.SERVERID1}</id>
	      <username>${env.USERID1}</username>
	      <password>${env.PWDID1}</password>
	    </server>
	    <server>
	      <id>${env.SERVERID2}</id>
	      <username>${env.USERID2}</username>
	      <password>${env.PWDID2}</password>
	    </server>
 	 </servers>


	<mirrors/>
	<proxies/>
	<profiles>
		<profile>
			<id>${env.PROFID}</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>

			<properties>
				<gpg.passphrase></gpg.passphrase>
			</properties>

			<repositories>
				<repository>
					<id>${env.REPOID1}</id>
					<name>${env.REPONAME1}</name>
					<url>${env.REPOURL1}</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
                      	<updatePolicy>always</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>${env.REPOID2}</id>
					<name>${env.REPONAME2}</name>
					<url>${env.REPOURL2}</url>
					<releases>
						<enabled>true</enabled>
                      	<updatePolicy>always</updatePolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
			</repositories>
            <pluginRepositories>
              <pluginRepository>
                  <id>${env.PLUGID1}</id>
                  <name>${env.PLUGNAME1}</name>
                  <url>${env.PLUGURL1}</url>
                  <releases>
                      <enabled>false</enabled>
                  </releases>
                  <snapshots>
                      <enabled>true</enabled>
                      <updatePolicy>always</updatePolicy>
                  </snapshots>
              </pluginRepository>
              <pluginRepository>
                  <id>${env.PLUGID2}</id>
                  <name>${env.PLUGNAME2}</name>
                  <url>${env.PLUGURL2}</url>
                  <releases>
                      <enabled>true</enabled>
                      <updatePolicy>always</updatePolicy>
                  </releases>
                  <snapshots>
                      <enabled>false</enabled>
                  </snapshots>
              </pluginRepository>
          </pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles/>
</settings> 

